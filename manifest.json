{
  "manifest_version": 2,

  "name": "ExecJS",
  "description": "Nothing special",
  "version": "1.0",
  "options_page": "config_page.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}